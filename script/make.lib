#!/bin/sh


test_exit() {
  status=$?
  if test $status -ne 0 
    then 
      exit 
  fi 
}

log="superdarn-toolkit.lib.2.11"
dtval=`date +%Y%m%d.%H%M`

$BINPATH/makeall -vb $CODEBASE/src.lib/base >> $LOGPATH/${log}.${dtval}
test_exit
$BINPATH/makeall -vb $CODEBASE/src.lib/general >> $LOGPATH/${log}.${dtval}
test_exit
$BINPATH/makeall -vb $CODEBASE/src.lib/analysis >> $LOGPATH/${log}.${dtval}
test_exit
cat $CODEBASE/src.lib/superdarn/mklst | $BINPATH/makeall -vb $CODEBASE/src.lib/superdarn stdin >> $LOGPATH/${log}.${dtval}
test_exit
$BINPATH/makeall -vb $CODEBASE/src.lib/imagery >> $LOGPATH/${log}.${dtval}








