#!/bin/sh




test_exit() {
  status=$?
  if test $status -ne 0 
    then 
      exit 
  fi 
}

log="superdarn-toolkit.bin.2.11"
dtval=`date +%Y%m%d.%H%M`

$BINPATH/makeall -vb $CODEBASE/src.bin/base > $LOGPATH/${log}.${dtval}
test_exit
$BINPATH/makeall -vb $CODEBASE/src.bin/general >> $LOGPATH/${log}.${dtval}
test_exit
$BINPATH/makeall -vb $CODEBASE/src.bin/superdarn >> $LOGPATH/${log}.${dtval} 
test_exit
$BINPATH/makeall -vb $CODEBASE/src.bin/imagery >> $LOGPATH/${log}.${dtval} 


test_exit









